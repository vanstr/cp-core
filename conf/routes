# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

POST        /login                       controllers.AuthorizationApi.login()
POST        /user                        controllers.AuthorizationApi.registerUser()
GET         /logout                      controllers.AuthorizationApi.logout()

GET         /dropboxAuthComplete         controllers.AuthorizationApi.dropboxAuthComplete(code: String)
GET         /driveAuthComplete           controllers.AuthorizationApi.driveAuthComplete(code: String)

# Map static resources from the /public folder to the /assets URL path
GET         /assets/*file                controllers.Assets.at(path="/public", file)